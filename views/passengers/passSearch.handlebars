<div id="home">
    <div class="container">
        <div class="row">
            <div class="col-8 offset-2">
                <h1><font color="white">{{msg}}</font></h1>
                <h2><font color="white">Welcome to ShredShare!</font></h2>
                <h3><font color="white">Available Rides:</font></h3>
                <ul id="example-list" class="list-group">
                    {{#each examples}}
                    <li data-id="{{this.id}}" class="list-group-item">
                        <a href="example/{{this.id}}">{{this.driverName}} has {{this.numSeats}} seats available and is
                            heading to {{this.destAddress}}</a>
                        <button class="btn btn-danger float-right delete">ｘ</button>
                    </li>
                    {{/each}}
                </ul>
                <a href="/">← Back To Home</a>
            </div>
        </div>
    </div>
</div>


<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>